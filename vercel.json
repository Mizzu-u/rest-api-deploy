{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    {
      "src": "/movies",
      "dest": "app.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
